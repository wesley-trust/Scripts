{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "environment": {
            "value": {
                "Settings": [
                    {
                        "name": "Production",
                        "suffix": "-p",
                        "virtualMachineSize": "Standard_D2s_v3"
                    },
                    {
                        "name": "Development",
                        "suffix": "-p",
                        "virtualMachineSize": "Standard_D2s_v3"
                    }
                ]
            }
        },
        "location": {
            "value": "uksouth"
        },
        "virtualMachineName": {
            "value": "Hostname"
        },
        "adminUsername": {
            "value": null
        },
        "adminPassword": {
            "value": null
        },
        "networkInterfaceName": {
            "value": "-nic"
        },
        "networkSecurityGroupName": {
            "value": "-nsg"
        },
        "virtualNetworkName": {
            "value": "-vnet"
        },
        "diagnosticsStorageAccountName": {
            "value": "diag"
        },
        "diagnosticsStorageAccountType": {
            "value": "Premium_LRS"
        },
        "diagnosticsStorageAccountKind": {
            "value": "Storage"
        },
        "addressPrefix": {
            "value": "10.0.0.0/16"
